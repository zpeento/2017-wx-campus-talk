<!--index.wxml-->
<view clas="page">
	<view class="page__bd">
		<block wx:for="{{tabs}}" wx:for-item="tab" wx:key="*this">
			<view class="block_space"></view>
			<view class="weui-form-preview">
				<view class="weui-form-preview__hd">
					<view class="weui-form-preview__item">
						<view class="weui-form-preview__label">{{tab.company}}</view>
					</view>
				</view>
				<view class="weui-form-preview__bd">
					<view class="weui-form-preview__item">
						<view class="weui-form-preview__label"><i class="fa fa-fw fa-calendar"></i>日期</view>
						<view class="weui-form-preview__value">{{tab.date}} {{tab.weekDay}}</view>
					</view>
					<view class="weui-form-preview__item">
						<view class="weui-form-preview__label"><i class="fa fa-fw fa-clock-o"></i>时间</view>
						<view class="weui-form-preview__value">{{tab.today}} {{tab.time}}</view>
					</view>
					<view class="weui-form-preview__item">
						<view class="weui-form-preview__label"><i class="fa fa-fw fa-book"></i>学校</view>
						<view class="weui-form-preview__value">{{tab.university}}</view>
					</view>
					<view class="weui-form-preview__item">
						<view class="weui-form-preview__label"><i class="fa fa-fw fa-map-marker"></i>地点</view>
						<view class="weui-form-preview__value">{{tab.location}}</view>
					</view>
				</view>
			</view>
		</block>
		<view class="weui-loadmore weui-loadmore_line">
			<view class="weui-loadmore__tips weui-loadmore__tips_in-line">暂无数据</view>
		</view>
	</view>
</view>
