<view class="page" wx:if="{{modules.msgPage.show}}">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="{{modules.msgPage.icon}}" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{modules.msgPage.title}}</view>
            <view class="weui-msg__desc">{{modules.msgPage.content}}</view>
        </view>
        <view class="weui-msg__opr-area" wx:if="{{false}}">
            <view class="weui-btn-area">
                <button class="weui-btn" bindtap="modules.msgPage.bindPrimaryTap" type="primary">
                    {{modules.msgPage.primary}}
                </button>
                <button class="weui-btn" bindtap="modules.msgPage.bindDefaultTap" type="default">
                    {{modules.msgPage.default}}
                </button>
            </view>
        </view>
        <view class="weui-msg__extra-area" wx:if="{{false}}">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link">爱试课</navigator>
                </view>
                <view class="weui-footer__text">Copyright © 2017</view>
            </view>
        </view>
    </view>
</view>